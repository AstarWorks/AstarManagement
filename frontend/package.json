{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .ts,.js,.vue",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix",
    "typecheck": "nuxi typecheck",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:visual": "playwright test e2e/tests/visual.spec.ts",
    "test:visual:update": "playwright test e2e/tests/visual.spec.ts --update-snapshots",
    "test:visual:ui": "playwright test e2e/tests/visual.spec.ts --ui",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "test-storybook --ci --url http://localhost:6006",
    "test-storybook:visual": "test-storybook --coverage --watchAll=false",
    "test-storybook:coverage": "test-storybook --coverage",
    "storybook:serve": "npx serve storybook-static -l 6006",
    "visual:init": "bun run visual-tests/scripts/baseline-manager.ts init",
    "visual:generate": "bun run visual-tests/scripts/baseline-manager.ts generate",
    "visual:generate:force": "bun run visual-tests/scripts/baseline-manager.ts generate --force",
    "visual:compare": "bun run visual-tests/scripts/baseline-manager.ts compare",
    "visual:compare:strict": "bun run visual-tests/scripts/baseline-manager.ts compare --threshold=0.05",
    "visual:report": "bun run visual-tests/scripts/baseline-manager.ts report",
    "visual:cleanup": "bun run visual-tests/scripts/baseline-manager.ts cleanup",
    "visual:update-baselines": "test-storybook --updateSnapshot",
    "visual:test": "build-storybook && test-storybook --url http://localhost:6006",
    "visual:test:comprehensive": "bun build-storybook && bun storybook:serve & wait-on http://localhost:6006 && bun test-storybook:visual && bun visual:compare",
    "visual:ci": "test-storybook --ci --url http://localhost:6006 --coverage",
    "visual:dev": "concurrently \"bun storybook\" \"wait-on http://localhost:6006 && bun visual:compare --threshold=0.15\"",
    "visual:baseline:review": "open visual-tests/diff/approval-report.html",
    "test:storybook:all": "concurrently \"bun storybook\" \"wait-on http://localhost:6006 && bun test-storybook\"",
    "test:storybook:debug": "test-storybook --debug",
    "test:storybook:watch": "test-storybook --watch",
    "test:coverage:merge": "nyc merge coverage coverage/merged-coverage.json",
    "test:coverage:report": "nyc report --reporter=html --reporter=text --reporter=lcov",
    "test:all": "bun run test:run && bun run test:e2e && bun run test-storybook:ci",
    "test:ci": "bun run lint && bun run typecheck && bun run test:all",
    "analyze": "nuxt analyze",
    "perf:lighthouse": "lhci autorun",
    "perf:lighthouse:mobile": "lhci autorun --collect.settings.preset=mobile",
    "perf:lighthouse:desktop": "lhci autorun --collect.settings.preset=desktop",
    "perf:bundle": "nuxt analyze",
    "perf:bundle:visualize": "nuxt analyze --open",
    "perf:k6": "k6 run k6/load-test.js",
    "perf:k6:stress": "k6 run k6/stress-test.js",
    "perf:k6:spike": "k6 run k6/spike-test.js",
    "perf:memory": "node scripts/memory-profiler.js",
    "perf:test": "vitest run tests/performance/",
    "perf:all": "bun run perf:lighthouse && bun run perf:bundle && bun run perf:test"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@nuxtjs/tailwindcss": "6",
    "@pinia/nuxt": "^0.11.1",
    "@tanstack/query-persist-client-core": "^5.81.2",
    "@tanstack/vue-query": "^5.62.11",
    "@tanstack/vue-virtual": "^3.13.12",
    "@vee-validate/rules": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/integrations": "^13.4.0",
    "@vueuse/nuxt": "^13.4.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dropzone-vue3": "^1.0.2",
    "jspdf": "^3.0.1",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.522.0",
    "nuxt": "^3.17.5",
    "papaparse": "^5.5.3",
    "radix-vue": "^1.9.17",
    "shadcn-nuxt": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "vue-virtual-scroller": "^1.1.2",
    "vuedraggable": "next",
    "web-vitals": "^5.0.3",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint-config": "^0.7.2",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tanstack/vue-query-devtools": "^5.62.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.3",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "axe-playwright": "^2.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^18.0.1",
    "lighthouse": "^12.6.1",
    "nyc": "^17.1.0",
    "playwright": "^1.53.1",
    "storybook": "^8.6.14",
    "storybook-dark-mode": "^4.0.2",
    "tailwindcss": "3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10",
    "wait-on": "^7.2.0"
  }
}
